model RefreshTokens {
    token     String   @unique
    user      Users    @relation(fields: [userId], references: [id])
    userId    Int      @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("refresh-tokens")
}

model Users {
    id           Int            @id @default(autoincrement())
    email        String         @unique
    password     String
    nickname     String
    refreshToken RefreshTokens?
}
